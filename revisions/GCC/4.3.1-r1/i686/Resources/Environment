#
# Modify "-march" to fit your architecture
#
export CFLAGS="-O2 -march=i686 -fomit-frame-pointer -pipe"
export CXXFLAGS="-O2 -march=i686 -fomit-frame-pointer -pipe"

export CPLUS_INCLUDE_PATH="${goboHeaders}/c++/4.3.1:${goboHeaders}${CPLUS_INCLUDE_PATH:+:$CPLUS_INCLUDE_PATH}"
export C_INCLUDE_PATH="${goboHeaders}${C_INCLUDE_PATH:+:$C_INCLUDE_PATH}"
